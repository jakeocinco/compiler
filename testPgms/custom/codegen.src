program LoopFib is
    variable loopVal : integer;
    variable val : integer;
    variable tempBool : bool;
begin
    val := 5;
    for(loopVal := 0; loopVal < val)
        tempBool := putInteger(loopVal);
        loopVal := loopVal + 1;
    end for;
    tempBool := putString("Done");
end program.
